<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ColorPrimary1">#64B5F6</Color>
    <Color x:Key="ColorPrimary2">#2979FF</Color>

    <Color x:Key="ColorBlue">#4D85BD</Color>
    <Color x:Key="ColorGreen">#7CAA2D</Color>
    <Color x:Key="ColorYellow">#F5E356</Color>
    <Color x:Key="ColorOrange">#CB6318</Color>

    <SolidColorBrush x:Key="BrushPrimary1" 
                     Color="{StaticResource ColorPrimary1}"/>

    <SolidColorBrush x:Key="BrushPrimary2"
                     Color="{StaticResource ColorPrimary2}" />

    <SolidColorBrush x:Key="MainBlue"
                     Color="{StaticResource ColorBlue}" />

    <SolidColorBrush x:Key="MainGreen"
                     Color="{StaticResource ColorGreen}" />

    <SolidColorBrush x:Key="MainYellow"
                     Color="{StaticResource ColorYellow}"/>

    <SolidColorBrush x:Key="MainOrange"
                     Color="{StaticResource ColorOrange}" />


</ResourceDictionary>